<view class="wrapper">
  <t-upload
    media-type="{{['image']}}"
    files="{{originFiles}}"
    gridConfig="{{gridConfig}}"
    bind:success="handleSuccess"
    bind:remove="handleRemove"
    bind:click="handleClick"
  />
</view>
<view class="btn">
    <t-button theme="primary" size="large" bindtap="sendImgsToDisplay">确认推送</t-button>
</view>